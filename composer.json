{
    "name": "froggdev/behat",
    "description": "Behat version to work with froggdev/behat-installer.",
    "keywords": ["symfony", "framework", "behat", "bdd", "frogg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marsiglietti Remy",
            "email": "admin@frogg.fr"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "autoload": {
        "psr-4": {
	          "Behat\\Behat\\":    					"behat/Behat/src/",
            "Behat\\Testwork\\": 					"behat/Testwork/src/",
						"Behat\\Symfony2Extension\\": "symfony2-extension/src/",
            "Behat\\Gherkin\\":  					"gherkin/src/",
						"Behat\\MinkExtension\\": 		"mink-extension/src/",
						"Behat\\Tests\\Transliterator\\": "transliterator/tests",	
            "Behat\\Mink\\":     				"mink/src/",
						"Behat\\Mink\\Driver\\": 		"mink-drivers/src/",
            "emuse\\BehatHTMLFormatter\\":"behat-html-formatter/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
