{
    "name": "froggdev/behat",
    "description": "Behat version to work with froggdev/behat-installer.",
    "keywords": ["symfony", "framework", "behat", "bdd", "frogg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marsiglietti Remy",
            "email": "admin@frogg.fr"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "autoload": {
        "psr-0": {
            "Behat\\Behat":    					"behat/src/",
            "Behat\\Testwork": 					"behat/src/",
						"Behat\\Symfony2Extension": "symfony2-extension/src/",
            "Behat\\Gherkin":  					"gherin/src/",
						"Behat\\MinkExtension": 		"mink-extension/src/",
						"Behat\\Tests\\Transliterator": "transliterator/tests"						
				},
        "psr-4": {

            "Behat\\Mink\\":     					"mink/src/",
            "Behat\\Mink\\Driver\\": 			"mink-browserkit-driver/src/",
						"Behat\\Mink\\Driver\\": 			"mink-goutte-driver/src/",
						"Behat\\Mink\\Driver\\": 			"mink-selenium2-driver/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
